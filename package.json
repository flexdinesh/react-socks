  "dependencies": {
    "react": "^16.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^1.0.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.8.0",
    "eslint-plugin-react": "^7.4.0",
    "jest-cli": "^23.6.0",
    "node-sass": "^4.7.2",
